output: release

## ===================================================
# 设置你的应用的
# Usage:
#
# 当配置为对象时，可以单独配置每个应用的信息
# apps:
#   <service name>: <entry file path>
#   <service name>:
#     path:  <entry file path>
#     view:  <view template path>
#     title: <service title>    # HTML TITLE
#
# Example:
#
# apps:
#   admin: src/admin
#   demo:
#     path: src/admin
#     view: src/admin/viewer.html
#     title: HTML TITLE
#
## ===================================================
#
apps:
  gmall: gmall
  demo : demo
#   t2:
#     path: src/services/demo
#   t3:
#     path: src/services/demo
#     view: false
#   t4:
#     path: src/services/demo/app.jsx
#     view: src/services/demo/app.tpl




# 清除历史缓存文件
#
clearCache: true


define:
  env: $NODE_ENV


node_modules:
  - xrosy
  - zals


# 设置别名
alias:
  admin: './src/admin'
  utils: './src/utils'