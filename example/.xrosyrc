
# 当配置为对象时，可以单独配置每个应用的信息
# service:
#   <service name>: <portal path>
#   <service name>:
#     path:  <portal path>
#     view:  <view path>
#     title: <service title>    # HTML TITLE
#
# service:
#   admin: src/admin
#   demo:
#     path: src/admin
#     view: src/admin/viewer.html
#     title: HTML TITLE
#
services:
  admin: src/services/admin/main.jsx
  portal:
    path: src/services/demo/app.jsx
    view: src/services/demo/app.tpl
  demo:
    path: src/services/demo
    view: false


output: build
# output:
#   path: _www
#   publicPath: https://static.zhonganbio.com/static

# 清除历史缓存文件
#
clearCache: true


define:
  env: $NODE_ENV


node_modules:
  - xrosy
  - zals


# 设置别名
alias:
  admin: './src/admin'
  utils: './src/utils'