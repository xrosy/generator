

# 当配置为字符时，设置应用目录的集合
#
# apps: src/apps


node_modules:
  - xrosy
  - zals


# 当配置为对象时，可以单独配置每个应用的信息
#
# apps:
#   admin: src/admin
#   demo:
#     path: src/admin
#     view: src/admin/viewer.html
#     title: HTML TITLE


# 设置别名
alias:
  admin: './src/admin'
  utils: './src/utils'



service:
  src/zals:
    - aa
    - bb