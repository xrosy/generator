{
  "presets": [
    ["minify", {
      "removeConsole": false,
      "removeDebugger": false,
      "keepFnName": true,
      "keepClassName": true
    }],

    ["@babel/preset-env"]
  ],

  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "absoluteRuntime": false,
      "corejs": "3",
      "helpers": true,
      "regenerator": true,
      "useESModules": false
    }],

    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],

    [ "@babel/plugin-proposal-class-properties", {
      "loose": true
    }]
  ]
}