

## ===================================================
# 设置你的应用的
# Usage:
#
# 当配置为对象时，可以单独配置每个应用的信息
# apps:
#   <service name>: <entry file path>
#   <service name>:
#     path:  <entry file path>
#     view:  <view template path>
#     title: <service title>    # HTML TITLE
#
# Example:
#
# apps:
#   admin: src/admin
#   demo:
#     path: src/admin
#     view: src/admin/viewer.html
#     title: HTML TITLE
#
## ===================================================

# 打包输出目录
built: ./release

server: ./server

apps:
  - src/app

# 清除历史缓存文件
#
clearCache: true


# 暂时未实现
#
# define:
#   env: $NODE_ENV
## ---------------------------------------------------

modules:
  - xrosy
  - zals

# 映射别名
#
alias:
  "app": "src/app"
  "app/*": "src/app/*"
  "gui": "src/gui"
  "library": "src/library"
  "utils": "src/utils"
